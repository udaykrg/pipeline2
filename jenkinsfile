pipeline{
  agent any
      stages {
          stage ("build") {
              step { 
                sh '''
                  echo "this is build stage"
                  '''
                  }
          stage ("deploy") {
              step { 
                sh '''
                  echo "this is deploy stage"
                  '''
                  }
           stage ("test") {
              step { 
                sh '''
                  echo "this is test stage"
                  '''
                  }
          }
  }

}
